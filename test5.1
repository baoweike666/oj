#include<iostream>
#include<stdio.h>
#include<stack>
#include<string.h>
#include<stdlib.h>
#include<queue>

using namespace std;


int tree[1000];
int find(int x)
{
    if(tree[x]==-1) return x;
    else return find(tree[x]);
}


int main()
{
    int n,m;
while(scanf("%d",&n)==1&&scanf("%d",&m)==1)
{
    for(int i=0;i<n;i++)
    {
        tree[i]=-1;
    }
    for(int i=0;i<m;i++)
    {
        int a,b;
        scanf("%d%d",&a,&b);
        a=find(a);
        b=find(b);
        if(a!=b) tree[a]=b;
    }
    int ans=0;
    for(int i=0;i<n;i++)
    {
        if(tree[i]==-1)
            ans++;
    }
printf("%d",ans-1);

}
    return 0;
}
