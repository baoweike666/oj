#include <iostream>
#include<algorithm>
#include<stdio.h>
#include<string.h>
using namespace std;



int main()
{
    int n;
    while(scanf("%d",&n)==1&&n!=0)
    {
       int a[50];
       int hash[1000];
       int t;
       memset(hash,0,100);
       for(int i=0;i<n;i++)
       scanf("%d",&a[i]);
        scanf("%d",&t);
       for(int i=0;i<n;i++)
       {
            printf("%d\n", a[i]);
           hash[a[i]]++;
            printf("%d\n", hash[a[i]]);
       }
       printf("%d",hash[t]);
    }
    return 0;
}

