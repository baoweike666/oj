#include <iostream>
#include<algorithm>
#include<stdio.h>
#include<string.h>
using namespace std;

int monthdays[2500][13];

class date
{
public:
    int year;
    int month;
    int day;
    void nextday()
    {
    day++;
    if(day==monthdays[year][month]+1)
    {
        day=1;
        month++;
    }
    if(month==13)
    {
        month=1;
        year++;
    }
    }
};

bool runyear(int year)
{
if((year%4==0&&year%100!=0)||(year%4==0&&year%400==0))
    return 1;
else
    return 0;
}


int main()
{
    for (int i=0;i<2500;i++)
    {
        monthdays[i][0]=0;//{0,31,28,31,30,31,30,31,31,30,31,30,31};
        monthdays[i][1]=31;
        monthdays[i][2]=28;
        monthdays[i][3]=31;
        monthdays[i][4]=30;
        monthdays[i][5]=31;
        monthdays[i][6]=30;
        monthdays[i][7]=31;
        monthdays[i][8]=31;
        monthdays[i][9]=30;
        monthdays[i][10]=31;
        monthdays[i][11]=30;
        monthdays[i][12]=31;
        if (runyear(i))
           monthdays[i][2]++;
    }

    date a[2];
    int days;
    scanf("%4d%2d%2d",&a[0].year,&a[0].month,&a[0].day);
    scanf("%4d%2d%2d",&a[1].year,&a[1].month,&a[1].day);
for(int i=0;i<1500;i++)
{
    days=i+1;
   if (a[0].year==a[1].year&&a[0].month==a[1].month&&a[0].day==a[1].day) break;
     a[0].nextday();
}
    printf("%d",days);
    return 0;
}
