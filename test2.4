#include <iostream>
#include<algorithm>
#include<stdio.h>
#include<string.h>
using namespace std;

int monthdays[2500][13];
char monthtoname[13][100]={"","January","February","March","April","May","June","July","August","September","October","November","December"};
char weektoname[8][100]={"Monday","Tuesday","Wednesday","Thursday","Friday","Saturday","Sunday"};
class date
{
public:
    int year;
    char monthname[100];
    int month;
    int day;
    void nextday()
    {
    day++;
    if(day==monthdays[year][month]+1)
    {
        day=1;
        month++;
    }
    if(month==13)
    {
        month=1;
        year++;
    }
    }
};

bool runyear(int year)
{
if((year%4==0&&year%100!=0)||(year%4==0&&year%400==0))
    return 1;
else
    return 0;
}




int main()
{
    for (int i=0;i<2500;i++)
    {
        monthdays[i][0]=0;//{0,31,28,31,30,31,30,31,31,30,31,30,31};
        monthdays[i][1]=31;
        monthdays[i][2]=28;
        monthdays[i][3]=31;
