        monthdays[i][8]=31;
        monthdays[i][9]=30;
        monthdays[i][10]=31;
        monthdays[i][11]=30;
        monthdays[i][12]=31;
        if (runyear(i))
           monthdays[i][2]++;
    }
    date a[2];
    int days;
    int t;
    while(scanf("%d%s%d",&a[0].day,a[0].monthname,&a[0].year))
    {
    a[1].year=2018;
    a[1].month=7;
    a[1].day=1;
for(int i=0;i<13;i++)
{
    if(strcmp(a[0].monthname,monthtoname[i])==0)
        a[0].month=i;
}
for(int i=0;i<150000;i++)
{
    days=i+1;
   if (a[0].year==a[1].year&&a[0].month==a[1].month&&a[0].day==a[1].day) break;
     a[0].nextday();
}
t=(7-(days+7)%7);
    printf("%d%s",days,weektoname[t]);
}
    return 0;
}

